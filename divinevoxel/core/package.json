 "license": "MIT",
 "devDependencies": {},
 "dependencies": {
  "@babylonjs/core": "^6.23.0",
  "@divinestar/binary": "^0.0.03",
  "@divinestar/utils": "^0.0.03",
  "@divinestar/shaders": "^0.0.03",
  "@divinestar/threads": "^0.0.03",
  "@divinestar/voxelspaces": "^0.0.03"
 },
 "main": "index.js",
 "publishConfig": {
  "access": "public"
 }
