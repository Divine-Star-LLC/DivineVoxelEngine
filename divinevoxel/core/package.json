{
 "name": "@divinevoxel/core",
 "version": "0.0.03",
 "module": "index.js",
 "types": "index.d.ts",
 "type": "module",
 "description": "TypeScript Voxel Engine - Uses Babylon.js",
 "keywords": [
  "voxel",
  "voxel engine",
